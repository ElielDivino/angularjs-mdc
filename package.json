{
  "name": "@fintechstudios/angularjs-mdc",
  "version": "0.2.1",
  "description": "Material Design Components Web for AngularJS",
  "main": "./dist/index",
  "scripts": {
    "lint": "eslint --color src",
    "build": "webpack --colors",
    "build:min": "cross-env MDC_ENV=production webpack -p --colors",
    "build:demos": "cross-env MDC_ENV=demos webpack -p --colors",
    "build:all": "npm run build && npm run build:min",
    "test": "cross-env MDC_ENV=test karma start karma.conf.js",
    "test:ci": "karma start karma.ci.conf.js",
    "serve": "cross-env MDC_ENV=development webpack-dev-server --content-base demos --inline --hot --host 0.0.0.0",
    "preversion": "npm run test -- --single-run",
    "version": "npm run build:all && git add -A dist",
    "postversion": "git push --follow-tags && npm publish --access public",
    "deploy:pages": "./ghpages-deploy.sh demos"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fintechstudios/angularjs-mdc.git"
  },
  "author": "Dylan Praul",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fintechstudios/angularjs-mdc/issues"
  },
  "homepage": "https://github.com/fintechstudios/angularjs-mdc#readme",
  "peerDependencies": {
    "angular": ">=1.5 <1.7"
  },
  "devDependencies": {
    "@material/animation": "^0.3.1",
    "@material/base": "^0.2.3",
    "@material/button": "^0.3.11",
    "@material/card": "^0.2.6",
    "@material/checkbox": "^0.4.2",
    "@material/dialog": "^0.3.5",
    "@material/drawer": "^0.5.4",
    "@material/elevation": "^0.1.11",
    "@material/fab": "^0.3.13",
    "@material/form-field": "^0.2.11",
    "@material/grid-list": "^0.2.8",
    "@material/icon-toggle": "^0.1.16",
    "@material/layout-grid": "^0.4.1",
    "@material/linear-progress": "^0.1.6",
    "@material/list": "^0.2.14",
    "@material/menu": "^0.4.3",
    "@material/radio": "^0.2.9",
    "@material/ripple": "^0.8.2",
    "@material/rtl": "^0.1.7",
    "@material/select": "^0.3.12",
    "@material/slider": "^0.2.2",
    "@material/snackbar": "^0.3.2",
    "@material/switch": "^0.1.11",
    "@material/tabs": "^0.2.5",
    "@material/textfield": "^0.3.3",
    "@material/theme": "^0.1.7",
    "@material/toolbar": "^0.4.4",
    "@material/typography": "^0.3.0",
    "angular": "^1.6.5",
    "angular-debounce": "^1.1.0",
    "angular-mocks": "^1.6.5",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-angularjs-annotate": "^0.8.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-angular": "^3.0.0",
    "eslint-plugin-mocha": "^4.11.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "watchify": "^3.9.0",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.5.0"
  }
}
